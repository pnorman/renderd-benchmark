psql -d gis -c 'CLUSTER planet_osm_roads USING "planet_osm_roads_pkey"' &
psql -d gis -c 'CLUSTER planet_osm_point USING "planet_osm_point_pkey"' &
psql -d gis -c 'CLUSTER planet_osm_line USING "planet_osm_line_pkey"' &
psql -d gis -c 'CLUSTER planet_osm_polygon USING "planet_osm_polygon_pkey"' &
wait
